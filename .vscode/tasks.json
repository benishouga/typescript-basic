{
  "version": "0.1.0",
  "command": "npm",
  "isShellCommand": true,
  "showOutput": "always",
  "suppressTaskName": true,
  "tasks": [
    {
      "taskName": "install",
      "args": [
        "install"
      ]
    },
    {
      "taskName": "compile",
      "args": [
        "run",
        "compile"
      ]
    },
    {
      "taskName": "_compile",
      "showOutput": "never",
      "args": [
        "run",
        "compile"
      ]
    },
    {
      "taskName": "update",
      "args": [
        "update"
      ]
    },
    {
      "taskName": "test",
      "args": [
        "run",
        "test"
      ]
    }
  ]
}