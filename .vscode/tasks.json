{
  "version": "0.1.0",
  "command": "npm",
  "isShellCommand": true,
  "showOutput": "always",
  "suppressTaskName": true,
  "tasks": [
    {
      "taskName": "install",
      "args": ["install"]
    },
    {
      "taskName": "compile",
      "args": ["run", "compile"],
      "isBuildCommand": true
    },
    {
      "taskName": "_compile",
      "showOutput": "never",
      "args": ["run", "compile"],
      "isBuildCommand": true
    },
    {
      "taskName": "update",
      "args": ["update"]
    },
    {
      "taskName": "test",
      "args": ["run", "test"]
    }
  ]
}
