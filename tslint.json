{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-console": [
            false
        ],
        "member-ordering": [
            false
        ],
        "trailing-comma": [
            true,
            {
                "multiline": "never",
                "singleline": "never"
            }
        ],
        "object-literal-sort-keys": false
    },
    "rulesDirectory": []
}